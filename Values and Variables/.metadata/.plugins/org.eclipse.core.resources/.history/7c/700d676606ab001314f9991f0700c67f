// Arrays

var muppetNames = ["kermit", "Miss Piggy","Gonzo","Rizzo" ]; //indexes 0,1,2,3

console.log(muppetNames[1]); //Outputs 2nd name in muppetNames to console

muppetNames[4] = "Fozzy"; //Sets the 5th name in the array to "Fozzy"
console.log(4); //Outputs 5th name in muppetNames to console
